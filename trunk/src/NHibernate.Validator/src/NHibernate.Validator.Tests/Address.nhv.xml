<?xml version="1.0" encoding="utf-8" ?>
<validator xmlns="urn:nhibernate-validator-0.1">
	<property name="blacklistedZipCode">
		<rules>
			<not-null/>
		</rules>
	</property>
	<property name="country">
		<rules>
			<length max="20"/>
			<not-null />
		</rules>
	</property>
	<property name="floor">
		<rules>
			<range min="-2" max="50" message="{floor.out.of.range} (escaping #{el})" />
		</rules>
	</property>
	<property name="Id">
		<rules>
			<min value="1" />
			<range max="2000" />
		</rules>
	</property>
	<property name="Line1" >
		<rules>
			<not-null/>
		</rules>
	</property>
	<property name="State">
		<rules>
			<not-null/>
			<length max="3"/>
		</rules>
	</property>

	<property name="Zip">
		<rules>
			<length max="5" message="{long}"/>
			<pattern regex="[0-9]+" />
			<not-null/>

		</rules>
	</property>
	<property name="InternalValid">
		<rules>
			<asserttrue />
		</rules>
	</property>
</validator>