<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="false" namespace="NHibernate.DomainModel" assembly="NHibernate.DomainModel">
  <class name="Simple" >
    <composite-id class="SimpleKey" name="Id">
      <key-property name="Number" type="Int32" column="number" />
      <key-property name="Year" type="DateTime" column="year"/>
    </composite-id>
    <property name="Name"/>
    <property name="Address" type="String" length="200" column="address" />
    <property name="Count" type="Int32" column="count_" not-null="true" unique="true"/>
    <property name="Date" type="DateTime" column="date_"/>
    <property name="Pay" />
  </class>
</hibernate-mapping>
