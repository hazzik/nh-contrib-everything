<?xml version="1.0" encoding="utf-8" ?>
<validator xmlns="urn:nhibernate-validator-0.1">
	<class name="Controller" assembly="NHibernate.Validator.Tests" namespace="NHibernate.Validator.Tests.CustomValidator">
		<property name="IP">
			<rule attribute="IPSubnetAttribute" assembly="NHibernate.Validator.Tests" namespace="NHibernate.Validator.Tests.CustomValidator">
				<param name="SubnetPrefix" value="192.168.2"/>
			</rule>
		</property>

		<property name="Name">
			<not-empty/>
			<not-null/>
		</property>
	</class>
</validator>
