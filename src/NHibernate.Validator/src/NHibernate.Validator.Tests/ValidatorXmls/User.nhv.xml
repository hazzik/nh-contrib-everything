<?xml version="1.0" encoding="utf-8" ?>
<validator xmlns="urn:nhibernate-validator-0.1">
	<class name="User" namespace="NHibernate.Validator.Tests" assembly="NHibernate.Validator.Tests">
		<property name="name">
			<rules>
				<not-null/>
			</rules>
		</property>
		<property name="email">
			<rules>
				<email />
			</rules>
		</property>
	</class>
</validator>
